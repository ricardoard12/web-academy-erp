<%@page import="academy.master.db.ListPackage"%><%@page import="java.util.List"%><%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%	request.setCharacterEncoding("utf-8");			ListPackage Pack = (ListPackage) request					.getAttribute("ListPackage");			List smslist = Pack.getClasslist();			int listcount = Pack.getListcount();			int nowpage = Pack.getPage();			int maxpage = Pack.getMaxpage();			int startpage = Pack.getStartpage();			int endpage = Pack.getEndpage();			//List roomlist = (List) request.getAttribute("roomlist");%><!DOCTYPE><html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><link href="./css/default.css" rel="stylesheet" type="text/css"><link href="./css/board.css" rel="stylesheet" type="text/css"><link href="./css/demos.css" rel="stylesheet"><link rel="stylesheet"	href="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css" /><title>전체 문자보기</title><script src="./js/jquery-1.9.1.js"></script><script src="./js/jquery-ui.custom.js"></script><script>	function detail(value) {		window.open ("./DetailSms.sms?sms_idx=" + value,"","");	};</script><style>tr:hover {	background: #FAECC5;}</style></head><body>	<!-- UI Object -->	<div id="wrap">		<p>#wrap</p>		<!-- header -->		<div id="header">			<jsp:include page="../page/header.jsp"></jsp:include>		</div>		<!-- //header -->		<!-- container -->		<div id="container">			<!-- snb -->			<div class="snb">				<jsp:include page="../page/snb.jsp"></jsp:include>			</div>			<!-- //snb -->			<!-- content -->			<div id="content">				<!-- 직원 목록 시작 -->				<!-- UI Object -->				<div id="dialog"></div>				<form action="./DeleteClass.master" method="post">					<input type="hidden" name="page" value="<%=nowpage%>">					<table cellspacing="0" border="1" summary="직원리스트"						class="tbl_type_list">						<caption>학급 목록</caption>						<!-- 						<colgroup> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 							<col width="8%"> -->						<!-- 						</colgroup> -->						<thead>							<tr>								<th scope="col"></th>								<th scope="col">ID</th>								<th scope="col">수신자 ID</th>								<th scope="col">수신자 이름</th>								<th scope="col">수신자 폰번호</th>								<th scope="col">송신자 폰번호</th>								<th scope="col">메세지</th>								<th scope="col">결과 코드</th>								<th scope="col">자동 전송</th>								<th scope="col">보낸 시간</th>							</tr>						</thead>						<tbody>							<%								if (listcount > 0) {				for (int i = 0; i < smslist.size(); i++) {					List list = (List) smslist.get(i);					String msg = "";							%>							<tr onclick="detail('<%=list.get(0)%>')">								<td><input type="checkbox" name="classlistchk"									value="<%=list.get(0)%>"></td>								<td><%=list.get(0)%></td>								<td><%=list.get(1)%></td>								<td><%=list.get(2)%></td>								<td><%=list.get(3)%></td>								<td><%=list.get(4)%></td>								<td>									<%										if (list.get(5).toString().length() > 10) {						msg = list.get(5).toString().substring(0, 10) + "...";					} else {						msg = list.get(5).toString();					}									%> <%=msg%></td>								<td><%=list.get(6)%></td>								<td><%=list.get(7)%></td>								<td><%=list.get(8)%></td>							</tr>							<%								}			}							%>						</tbody>					</table>					<!-- //UI Object -->					<div align="right">						<input type="button" name="createClass" value="none"							onclick="location.href='./CreateClass.master'">					</div>				</form>				<div class="paginate_complex" align="center">					<%						if (nowpage <= 1) {					%>					<a href="#" class="direction prev"><span></span><span></span>처음</a>					<%						} else {					%>					<a href="./AllMessage.sms?page=<%=nowpage - 1%>"						class="direction prev"><span></span>이전</a>					<%						}					%>					<%						for (int a = startpage; a <= endpage; a++) {				if (a == nowpage) {					%><strong><%=a%></strong>&nbsp;<%						} else {					%><a href="./AllMessage.sms?page=<%=a%>"><%=a%></a>					<%						}			}					%>					<%						if (nowpage >= maxpage) {					%><a href="#" class="direction next">끝<span></span><span></span></a>					<%						} else {					%><a href="./AllMessage.sms?page=<%=nowpage + 1%>"						class="direction next">다음 </a>					<%						}					%>				</div>				<!-- //수강생 관리 끝 -->				<div id="insertClassDialog-form" title="강의실을 넣으세요."></div>			</div>			<!-- //content -->		</div>		<!-- //container -->		<!-- footer -->		<div id="footer">			<jsp:include page="../page/footer.jsp"></jsp:include>		</div>		<!-- //footer -->	</div>	<!-- //UI Object --></body></html>