<%@page import="java.util.List"%><%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%	request.setCharacterEncoding("utf-8");			List sbiList = (List) request.getAttribute("sbi");%><!DOCTYPE><html><head><script src="./js/jquery-1.9.1.js"></script><script src="./js/jquery-ui.custom.js"></script><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><title>포인트 조회</title><link href="./css/default.css" rel="stylesheet" type="text/css"><link href="./css/board.css" rel="stylesheet" type="text/css"><link href="./css/demos.css" rel="stylesheet"><link rel="stylesheet"	href="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css" /><script>	$(function() {		creadits="<%=sbiList.get(4)%>	";		go = false;		if (creadits != "00") {			go = confirm("포인트가 부족합니다. 포인트 충전으로 바로 이동 하시겠습니까?");		}		if (go) {			window.open("http://www.coolsms.co.kr");			window.close();		} else {			return null;		}	});</script><style>#cash_table {	width: 100%;	height: 100%;	margin: 0;	font-size: 30px;	border: solid 1px;}#cash_table th {	background: #F0F0F0;	width: 60%;	font-size: 100%;}#cash_table td {	background: #F6F6F6;	width: 40%;	text-align: center;}</style></head><body>	<table id="cash_table">		<tr>			<th>잔액</th>			<td><%=sbiList.get(0)%> 원</td>		</tr>		<tr>			<th>적립포인트</th>			<td><%=sbiList.get(1)%> pt</td>		</tr>		<tr>			<th>물방울 포인트</th>			<td><%=sbiList.get(2)%> mpt</td>		</tr>		<tr>			<th>발송가능 갯수</th>			<td><%=sbiList.get(3)%> 개</td>		</tr>		<tr style="height: 20px">			<td colspan="2"><input type="button" value="창닫기"				onclick="window.close()"></td>		</tr>	</table></body></html>